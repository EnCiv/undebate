    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@sap_oss/node-socketio-stream": "^1.0.3",
    "classnames": "^2.3.1",
    "cloudinary": "^1.27.1",
    "color": "^4.1.0",
    "colors": "^1.4.0",
    "csvtojson": "^2.0.10",
    "device": "^0.3.12",
    "get-youtube-id": "^1.0.1",
